{
  "name": "@yobta/stores",
  "version": "0.1.11",
  "description": "An experimental module creatd for an education purpose",
  "keywords": [
    "experiment"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "lint": "eslint src",
    "size-limit": "size-limit",
    "test": "vitest src --coverage --run",
    "test:watch": "vitest src --coverage",
    "typecheck": "tsc --noEmit"
  },
  "author": "Sergei Droganov <sergei@droganov.ru>",
  "license": "MIT",
  "repository": "yobta/stores",
  "sideEffects": false,
  "type": "module",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "react": [
        "./lib/adapters/react/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./lib/index.js",
    "./react": "./lib/adapters/react/index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^14.0.0 || >=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@logux/eslint-config": "^47.2.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^28.1.8",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^16.11.66",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitest/coverage-c8": "^0.24.3",
    "c8": "^7.12.0",
    "check-dts": "^0.5.6",
    "clean-publish": "^2.2.0",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-unicorn": "36.0.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "happy-dom": "^5.4.0",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "size-limit": "^8.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vitest-globals/recommended",
      "@logux/eslint-config/esm"
    ],
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "vitest-globals/env": true
        }
      }
    ],
    "rules": {
      "@typescript-eslint/unified-signatures": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "symbol-description": "off",
      "consistent-return": "off",
      "jest/no-deprecated-functions": "off",
      "camelcase": "off"
    },
    "globals": {
      "jsdom": false
    }
  },
  "eslintIgnore": [
    "**/errors.ts",
    "vitest.config.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
